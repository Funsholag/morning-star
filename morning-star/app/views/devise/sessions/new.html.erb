<img id="clouds" alt="" class="background-image responsive-img" src="/clouds-2354580_1920.jpg">
<div class="border-form-div">
  <h2 class= "logInTitle">Log in</h2>
  <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
    <%= f.email_field :email, class: "form-control center responsive", placeholder: "Email", :autofocus => true %>
    <%= f.password_field :password, autocomplete: "off", class: "form-control center responsive", placeholder: "Password" %>
    <% if devise_mapping.rememberable? -%>
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    <% end -%>
    <span class= "logInBtn">
      <%= f.submit "Log in"%>
    </span>
  <% end %>

<%= render "devise/shared/links" %>
<br><br>
</div>
